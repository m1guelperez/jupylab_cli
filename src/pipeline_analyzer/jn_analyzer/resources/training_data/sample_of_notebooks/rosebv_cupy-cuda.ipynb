   "source": [
    "import cupy as cp\n",
    "\n",
    "x = cp.arange(6, dtype='f').reshape(2, 3)\n",
    "y = cp.arange(3, dtype='f')\n",
    "kernel = cp.ElementwiseKernel(\n",
    "    'float32 x, float32 y', 'float32 z',\n",
    "    '''if (x - 2 > y) {\n",
    "        z = x * y;\n",
    "    } else {\n",
    "        z = x + y;\n",
    "    }''',\n",
    "    'my_kernel')\n",
    "kernel(x, y)"
   ]
